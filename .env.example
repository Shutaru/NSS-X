# NSS X Environment Configuration
# Copy this file to .env and fill in your values

# Database Configuration
NSS_DB_HOST=localhost
NSS_DB_PORT=5432
NSS_DB_NAME=nss_x
NSS_DB_USER=postgres
NSS_DB_PASSWORD=your_password_here

# Data Paths
NSS_DATA_RAW=./01_data/raw
NSS_DATA_PROCESSED=./01_data/processed
NSS_DATA_GIS=./01_data/gis

# API Configuration (if needed)
NSS_API_HOST=0.0.0.0
NSS_API_PORT=8000

# Logging
NSS_LOG_LEVEL=INFO
